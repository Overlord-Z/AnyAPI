{
  "id": "canny",
  "name": "Canny API",
  "description": "Canny API for feature request and feedback management",
  "version": "1.0",
  "category": "Feedback",
  "categoryColor": "#7C3AED",
  "tags": ["feedback", "features", "roadmap", "voting"],
  "icon": "ðŸ’¡",
  "baseUrl": "https://canny.io/api/v1",
  "authType": "ApiKey",
  "ui": {
    "brandColor": "#7C3AED",
    "accentColor": "#8B5CF6",
    "logo": "https://assets.canny.io/5e6458a0849e16cb4aad3f7ef1cb8964adced47c/logo-icon.png",
    "gradient": "linear-gradient(135deg, #7C3AED 0%, #8B5CF6 100%)",
    "textColor": "#ffffff"
  },
  "defaultHeaders": {
    "Accept": "application/json",
    "Content-Type": "application/json"
  },
  "requiredSecrets": [
    {
      "key": "apiKey",
      "displayName": "API Key",
      "description": "Canny API key",
      "placeholder": "your-api-key",
      "isRequired": true,
      "helpUrl": "https://developers.canny.io/api-reference#authentication"
    }
  ],
  "authFieldMapping": {
    "ApiKey": {
      "queryParam": "apiKey"
    }
  },
  "customSettings": {},
  "sampleEndpoints": [
    {
      "method": "GET",
      "endpoint": "/boards/list",
      "description": "List all boards",
      "category": "Boards"
    },
    {
      "method": "GET",
      "endpoint": "/posts/list",
      "description": "List posts",
      "category": "Posts"
    },
    {
      "method": "POST",
      "endpoint": "/posts/create",
      "description": "Create a new post",
      "category": "Posts"
    },
    {
      "method": "GET",
      "endpoint": "/comments/list",
      "description": "List comments",
      "category": "Comments"
    },
    {
      "method": "GET",
      "endpoint": "/votes/list",
      "description": "List votes",
      "category": "Votes"
    },
    {
      "method": "GET",
      "endpoint": "/users/list",
      "description": "List users",
      "category": "Users"
    }
  ],
  "paginationDetails": {
    "type": "Simple",
    "supportsPageSize": true,
    "limitParameter": "limit",
    "defaultLimit": 10,
    "maxLimit": 100
  },
  "documentation": {
    "url": "https://developers.canny.io/",
    "quickStart": "https://developers.canny.io/api-reference"
  }
}